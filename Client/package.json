{
    "name": "aframe-project",
    "version": "1.0.0",
    "description": "This Web-Application will visualize Data in a Virtual Environment",
    "main": "index.tsx",
    "scripts": {
        "build": "webpack --watch",
        "styles": "node-sass --watch --include-path scss ./src/stylesheets/scss/main.scss ./src/stylesheets/css/main.css",
        "server": "./node_modules/http-server/bin/http-server ./dist",
        "start": "concurrently \"npm run build\" \"npm run styles\" \"npm run server\""
    },
